{
  "name": "test-jest-ts",
  "version": "0.0.1",
  "description": "testing with jest in typescript",
  "license": "UNLICENSED",
  "scripts": {
    "ci": "rm -rf node_modules && yarn install --frozen-lockfile",
    "preinstall": "sh ./scripts/preinstall.sh",
    "prettier-check": "prettier --check .",
    "prettier-write": "prettier --write .",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix && yarn lint",
    "lint-pretty": "yarn lint && yarn prettier-check",
    "lint-fix-pretty": "yarn lint-fix && yarn prettier-write",
    "test": "jest",
    "test-grep": "jest --testNamePattern ",
    "test-watch": "jest --watch "
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "axios": "^0.24.0",
    "dayjs": "^1.10.7",
    "eslint": "^7.32.0",
    "eslint-plugin-jest": "^24.7.0",
    "jest": "^27.3.1",
    "jest-html-reporters": "^2.1.6",
    "jest-junit": "^13.0.0",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.4"
  }
}
